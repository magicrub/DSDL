# Copy of MAVLink msg
# <message id="10001" name="UAVIONIX_ADSB_OUT_CFG">
# <description>Static data to configure the ADS-B transponder (send within 10 sec of a POR and every 10 sec thereafter)</description>


# <field type="uint32_t" name="ICAO">Vehicle address (24 bit)</field>
uint32 ICAO

# <field type="char[9]" name="callsign">Vehicle identifier (8 characters, null terminated, valid characters are A-Z, 0-9, " " only)</field>
uint8[9] callsign

# <field type="uint8_t" name="emitterType" enum="ADSB_EMITTER_TYPE">Transmitting vehicle type. See ADSB_EMITTER_TYPE enum</field>
# <description>ADSB classification for the type of vehicle emitting the transponder signal</description>
uint5 ADSB_EMITTER_TYPE_NO_INFO = 0
uint5 ADSB_EMITTER_TYPE_LIGHT = 1
uint5 ADSB_EMITTER_TYPE_SMALL = 2
uint5 ADSB_EMITTER_TYPE_LARGE = 3
uint5 ADSB_EMITTER_TYPE_HIGH_VORTEX_LARGE = 4
uint5 ADSB_EMITTER_TYPE_HEAVY = 5
uint5 ADSB_EMITTER_TYPE_HIGHLY_MANUV = 6
uint5 ADSB_EMITTER_TYPE_ROTOCRAFT = 7
uint5 ADSB_EMITTER_TYPE_UNASSIGNED = 8
uint5 ADSB_EMITTER_TYPE_GLIDER = 9
uint5 ADSB_EMITTER_TYPE_LIGHTER_AIR = 10
uint5 ADSB_EMITTER_TYPE_PARACHUTE = 11
uint5 ADSB_EMITTER_TYPE_ULTRA_LIGHT = 12
uint5 ADSB_EMITTER_TYPE_UNASSIGNED2 = 13
uint5 ADSB_EMITTER_TYPE_UAV = 14
uint5 ADSB_EMITTER_TYPE_SPACE = 15
uint5 ADSB_EMITTER_TYPE_UNASSGINED3 = 16
uint5 ADSB_EMITTER_TYPE_EMERGENCY_SURFACE = 17
uint5 ADSB_EMITTER_TYPE_SERVICE_SURFACE = 18
uint5 ADSB_EMITTER_TYPE_POINT_OBSTACLE = 19
uint5 emitterType

# <field type="uint8_t" name="aircraftSize" enum="UAVIONIX_ADSB_OUT_CFG_AIRCRAFT_SIZE">Aircraft length and width encoding (table 2-35 of DO-282B)</field>
# <description>Definitions for aircraft size</description>
uint4 UAVIONIX_ADSB_OUT_CFG_AIRCRAFT_SIZE_NO_DATA = 0
uint4 UAVIONIX_ADSB_OUT_CFG_AIRCRAFT_SIZE_L15M_W23M = 1
uint4 UAVIONIX_ADSB_OUT_CFG_AIRCRAFT_SIZE_L25M_W28P5M = 2
uint4 UAVIONIX_ADSB_OUT_CFG_AIRCRAFT_SIZE_L25_34M = 3
